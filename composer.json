{
    "name": "jackalopelabs/jackalopes-wp",
    "description": "3D first-person shooter game for WordPress built with React Three Fiber and ThreeJS",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mason Lawlor",
            "email": "mason@jackalope.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "composer/installers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Jackalopes\\WP\\": "src/"
        },
        "classmap": [
            "includes/",
            "admin/"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "suggest": {
        "jackalopelabs/jackalopes-server": "Required for multiplayer functionality"
    }
} 